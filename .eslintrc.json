{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {    
        "semi": ["warn", "always"],    "quotes": ["warn", "double"],    "no-var": "error"    
    }
}
